# =============================================================================
# Environment Variables Template
# =============================================================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!
# =============================================================================

# Google Cloud Project
GOOGLE_CLOUD_PROJECT=your-project-id
GCP_PROJECT_ID=your-project-id
GCP_REGION=asia-southeast2

# Pub/Sub
PUBSUB_TOPIC=ecommerce-events
PUBSUB_SUBSCRIPTION=ecommerce-events-sub

# Cloud Storage
GCS_BUCKET=your-project-id-dev-raw-data
GCS_PREFIX=incoming/

# BigQuery
BIGQUERY_DATASET=ecommerce_dw
BIGQUERY_TABLE=raw_events

# Data Generator
NUM_EVENTS=1000
BATCH_SIZE=100

# Airflow
AIRFLOW_UID=50000
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__DATABASE__SQL_ALCHEMY_CONN=postgresql+psycopg2://airflow:airflow@postgres/airflow
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here
AIRFLOW__WEBSERVER__SECRET_KEY=your-secret-key-here

# dbt
DBT_PROJECT_DIR=src/dbt
DBT_TARGET=dev

# =============================================================================
# SECURITY NOTES
# =============================================================================
# 1. Never commit .env file to Git
# 2. Never commit service account JSON keys
# 3. Use GitHub Secrets for CI/CD pipelines
# 4. Rotate keys regularly in production
# =============================================================================
